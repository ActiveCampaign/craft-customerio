{
  "name": "wildbit/craft-customerio",
  "description": "Send newsletter subscribers to Customer.io",
  "type": "craft-plugin",
  "version": "1.1.0",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "customer.io"
  ],
  "support": {
    "docs": "https://github.com/activecampaign/craft-customerio/blob/master/README.md",
    "issues": "https://github.com/activecampaign/craft-customerio/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "ActiveCampaign",
      "homepage": "https://activecampaign.com"
    }
  ],
  "require": {
    "craftcms/cms": "^3.0.0",
    "matt-west/craft-recaptcha": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "wildbit\\customerio\\": "src/"
    }
  },
  "extra": {
    "name": "Customer.io",
    "handle": "customerio",
    "developer": "ActiveCampaign",
    "developerUrl": "https://activecampaign.com",
    "documentationUrl": "https://github.com/activecampaign/craft-customerio/blob/master/README.md",
    "changelogUrl": "https://raw.githubusercontent.com/activecampaign/craft-customerio/master/CHANGELOG.md",
    "components": {
      "subscribers": "wildbit\\customerio\\services\\Subscribers"
    },
    "class": "wildbit\\customerio\\Customerio"
  }
}
